package interop;

import rdl.interop.*;

public class RDL extends RDLInterface{

	public RDL(){
		super();
	}
	
	public Relation defRel(String name, Object... args){
		
		return (Relation)super.defRel(name, args);
		
	}
	
	public Rule defRule(String name, Object... args){
		return (Rule)super.defRule(name, args);
	}
	
}
